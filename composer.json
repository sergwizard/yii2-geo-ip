{
    "name": "sergwizard/yii2-geo-ip",
    "description": "Yii 2 GeoIP extension. Returns country, city, lat, lng of current or specified IP (uses MaxMind's GeoIP2 databases)",
    "keywords": ["geoip", "geolocation", "maxmind", "yii2"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Serg Coder",
            "email": "coder3000web@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.0",
        "maxmind-db/reader": "~1.0",
        "sergwizard/maxmind-geolite2-db": "*"
        
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "sergwizard\\geoIp\\": "src/"
        }
    }
}
